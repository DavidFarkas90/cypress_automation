{
  "name": "cypress-workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "remove": "docker rmi --force cy-img:2.0",
    "build": "docker build -t cy-img:2.0 .",
    "dock-win": "docker run -it --name cy-cont -v %cd%:/e2e -w /e2e --entrypoint=cypress cy-img:2.0 run --reporter mochawesome --reporter-options reportDir=reports,reportFilename=practice_report_2 --spec 'cypress/integration/examples/practice.js'",
    "dock-linux": "docker run -it -v ${PWD}:/e2e -w /e2e --entrypoint=cypress cypress/included:6.1.0 run --reporter mochawesome --reporter-options reportDir=reports,reportFilename=practice_report_2 --spec 'cypress/integration/examples/practice.js'",
    "cypress:open": "cypress open",
    "cy:first_test": "npx cypress run --spec 'cypress/integration/my_first_test.js'",
    "cy:mobile": "npx cypress run --headless --browser chrome --spec 'cypress/integration/mobileAddToFavorites.spec.js'",
    "cy:practice": "npx cypress run --spec 'cypress/integration/examples/practice.js'",
    "cy:api": "npx cypress run --spec 'cypress/integration/examples/practice_api.js'",
    "cy:chrome": "npx cypress run --browser chrome --spec 'cypress/integration/my_first_test.js'",
    "cy:headless": "npx cypress run --headless --browser chrome --spec 'cypress/integration/my_first_test.js'",
    "cy:reporter": "npx cypress run --reporter mochawesome --reporter-options reportDir=reports,reportFilename=practice_report --spec 'cypress/integration/examples/practice.js'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress-file-upload": "^5.0.2",
    "cypress-xpath": "^1.6.1",
    "eslint": "^7.14.0",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1"
  },
  "dependencies": {
    "chai-colors": "^1.0.1"
  }
}
